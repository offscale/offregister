{
  "name": "Service",
  "description": "Offregister strategy for Odoo",
  "version": "0.0.1",
  "provider": {
    "$ref": "env:OFFAUTH_JSON|offutils.str_from_file | json.loads"
  },
  "register": {
    "/unclustered/OpenEdX4": [
      {
        "module": "offregister-postgres",
        "kwargs": {
          "cluster": false
        },
        "type": "fabric"
      },
      {
        "module": "offregister-odoo",
        "kwargs": {
          "DNS_NAME": "business.offscale.io"
        },
        "type": "fabric"
      },
      {
        "module": "offregister-certbot",
        "cluster_name": "certbot0",
        "args": [],
        "kwargs": {
          "domains": [
            "business.offscale.io"
          ],
          "email": "samuel@offscale.io"
        },
        "type": "fabric",
        "_run_cmds": {
          "op": ">",
          "val": 1
        }
      }
    ]
  },
  "purpose": [
    "odoo"
  ],
  "etcd_server": "http://localhost:2379",
  "default_pick": "first"
}
