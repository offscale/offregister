{
  "name": "REPLACEME",
  "description": "Default REPLACEME strategy",
  "version": "0.0.1",
  "provider": {
    "$ref": "env:OFFAUTH_JSON|offutils.str_from_file | json.loads"
  },
  "register": {
    "/fantom/devbox1": [
      {
        "module": "offregister-taiga",
        "kwargs": {
          "LISTEN_PORT": 443,
          "SERVER_NAME": null,
          "TAIGA_ROOT": "/var/www/taiga"
        },
        "type": "fabric"
      }
    ]
  },
  "_comments": [
    {
      "module": "offregister-bootstrap",
      "type": "fabric"
    },
    {
      "module": "offregister-taiga",
      "kwargs": {
        "LISTEN_PORT": 443,
        "SERVER_NAME": null,
        "TAIGA_ROOT": "/var/www/taiga"
      },
      "type": "fabric"
    },
    {
      "module": "offregister-docker",
      "type": "fabric"
    },
    {
      "module": "offregister-go",
      "type": "fabric"
    },
    {
      "module": "offregister-inline",
      "kwargs": {
        "run": [
          "command -v glide || curl https://glide.sh/get | sh",
          "go get -d github.com/andrecronje/evm/cmd/evm",
          "cd $GOPATH/src/github.com/andrecronje/evm      && glide install",
          "go get -d github.com/andrecronje/lachesis/cmd/lachesis",
          "cd $GOPATH/src/github.com/andrecronje/lachesis && glide install"
        ]
      },
      "type": "fabric"
    },
    {
      "module": "offregister-inline",
      "kwargs": {
        "run": [
          "$GOPATH/src/github.com/andrecronje/evm/scripts/build.bash"
        ]
      },
      "type": "fabric"
    },
    {
      "module": "offregister-hook",
      "kwargs": {
        "HOOK_IP": "127.0.0.1",
        "HOOK_PORT": 9000,
        "HOOK_VERBOSE": true,
        "HOOK_HOOKS_JSON": [
          {
            "id": "crossbuild-webhook",
            "execute-command": "$GOPATH/src/github.com/andrecronje/evm/scripts/build.bash",
            "command-working-directory": "$GOPATH/src/github.com/andrecronje/evm",
            "include-command-output-in-response": true,
            "pass-environment-to-command": [
              {
                "source": "string",
                "envname": "GOPATH",
                "name": "$GOPATH"
              },
              {
                "source": "string",
                "envname": "PATH",
                "name": "$PATH:/usr/local/go/bin:$GOPATH/bin"
              }
            ]
          }
        ],
        "NGINX_CONF": "taiga"
      },
      "run_cmds": {
        "op": "in",
        "val": [
          "configure_nginx1"
        ]
      },
      "type": "fabric"
    },
    {
      "module": "offregister-gopherci",
      "type": "fabric"
    },
    {
      "module": "offregister-gocd",
      "type": "fabric"
    }
  ],
  "purpose": [
    "any-cluster"
  ],
  "etcd_server": "http://localhost:2379",
  "default_pick": "first"
}
