{
  "name": "Web",
  "description": "Offregister strategy for web",
  "version": "0.0.1",
  "provider": {
    "$ref": "env:OFFAUTH_JSON|offutils.str_from_file | json.loads"
  },
  "register": {
    "/unclustered/OpenEdX4": [
      {
        "module": "offregister-app-push",
        "kwargs": {
          "GIT_DIR": "/var/www/static/hotel-kiosk-dist",
          "GIT_REPO": "https://github.com/SamuelMarks/hotel-kiosk-dist",
          "use_sudo": true,
          "node_sudo": false,
          "app_name": "kiosk",
          "NGINX_PORT": 80,
          "DESCRIPTION": "hotel-kiosk frontend",
          "DNS_NAMES": [
            "kiosk.offscale.io"
          ],
          "WWWROOT": "/var/www/static/hotel-kiosk-dist/dist",
          "WWWPATH": "/",
          "nginx": true
        },
        "type": "fabric"
      }
    ]
  },
  "purpose": [
    "any-cluster"
  ],
  "etcd_server": "http://localhost:2379",
  "default_pick": "first"
}
