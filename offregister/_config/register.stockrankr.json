{
  "name": "Stockrankr",
  "description": "Offregister strategy for Stockrankr",
  "version": "0.0.2",
  "env": {
    "OFFAUTH_JSON": {
      "$ref": "env:OFFAUTH_JSON"
    }
  },
  "provider": {
    "$ref": "env:OFFAUTH_JSON|offutils.str_from_file | json.loads"
  },
  "test_pkg": {
    "$ref": "pkg_file:offregister::_config::auth.sample.json"
  },
  "register": {
    "/unclustered/any-cluster-ubuntu1604-2-Glaucoma1": [
      {
        "module": "offregister-git",
        "kwargs": {
          "GIT_DIR": "/var/www/static/stockrankr-rest-api",
          "GIT_REPO": "https://github.com/stockrankr/stockrankr-rest-api",
          "GIT_BRANCH": "ben-company-details",
          "skip_reset": false,
          "use_sudo": true
        },
        "type": "fabric"
      },
      {
        "module": "offregister-stockrankr",
        "kwargs": {
          "STOCKRANKR_ROOT": "/var/www/static",
          "TRADING_DATA_STOCK_ROOT": "/mnt/Stocks",
          "TRADING_DATA_ROOT": "/mnt/ParsedStocks"
        },
        "type": "fabric"
      },
      {
        "module": "offregister-app-push",
        "kwargs": {
          "DAEMON_ENV": {
            "DEFAULT_ADMIN_EMAIL": {
              "$ref": "env:DEFAULT_ADMIN_EMAIL"
            },
            "DEFAULT_ADMIN_PASSWORD": {
              "$ref": "env:DEFAULT_ADMIN_PASSWORD"
            },
            "$$ENV_JSON_FILE": "/var/www/static/stockrankr-rest-api/bootstrap/env.json",
            "STRIPE_S_KEY": {
              "$ref": "env:STRIPE_S_KEY"
            }
          },
          "GIT_DIR": "/var/www/static/stockrankr-rest-api",
          "GIT_REPO": "https://github.com/stockrankr/stockrankr-rest-api",
          "GIT_BRANCH": "ben-company-details",
          "service_name": "stockrankr-rest-api",
          "skip_reset": false,
          "destroy_node_modules": true,
          "use_sudo": true,
          "node_sudo": false,
          "node_version": "8.9.4",
          "ExecStart": "/bin/bash -c 'PATH={home_dir}/n/bin:$PATH {home_dir}/n/bin/node main.js'",
          "RDBMS_URI": [
            "postgres://",
            {
              "$ref": "env:DB_USER"
            },
            ":",
            {
              "$ref": "env:DB_PASS"
            },
            "@",
            "localhost",
            "/",
            {
              "$ref": "env:DB_NAME"
            }
          ],
          "npm_global_packages": [
            "typings",
            "mocha",
            "typescript",
            "bunyan"
          ],
          "post_npm_step": "tsc",
          "nginx": true,
          "app_name": "stockrankr",
          "DESCRIPTION": "stockrankr frontend and backend",
          "DNS_NAMES": [
            "stockrankr.complicated.io"
          ],
          "PROXY_ROUTE": "/api",
          "PROXY_PASS": "http://localhost:5466",
          "REST_API_PORT": 5466,
          "NGINX_PORT": 80,
          "WWWROOT": "/var/www/static/stockrankr-web-frontend-dist/dist",
          "WWWPATH": "/",
          "___nginx_secure": "certbot",
          "___https_cert_email": "samuel@offscale.io",
          "Environments": "\nEnvironment=TRADING_DATA_PATHS=/data/Indices.csv:"
        },
        "____run_cmds": {
          "op": "==",
          "val": 0
        },
        "type": "fabric"
      },
      {
        "module": "offregister-app-push",
        "kwargs": {
          "GIT_DIR": "/var/www/static/stockrankr-web-frontend-dist",
          "GIT_REPO": "https://github.com/stockrankr/stockrankr-web-frontend-dist",
          "use_sudo": true,
          "node_sudo": false,
          "nginx": false
        },
        "type": "fabric"
      }
    ]
  },
  "_comments": [
    {
      "module": "offregister-redis",
      "type": "fabric"
    },
    {
      "module": "offregister-postgres",
      "kwargs": {
        "create": [
          {
            "user": {
              "$ref": "env:DB_USER"
            },
            "password": {
              "$ref": "env:DB_PASS"
            },
            "dbname": {
              "$ref": "env:DB_NAME"
            }
          }
        ],
        "users": [
          {
            "name": {
              "$ref": "env:DB_USER"
            },
            "password": {
              "$ref": "env:DB_PASS"
            }
          }
        ],
        "dbs": [
          {
            "$ref": "env:DB_NAME"
          }
        ],
        "cluster": false
      },
      "type": "fabric"
    }
  ],
  "purpose": [
    "any-cluster"
  ],
  "etcd_server": "http://localhost:2379",
  "default_pick": "first"
}
