{
  "name": "Default",
  "description": "Default register strategy",
  "version": "0.0.1",
  "provider": {
    "options": [
      {
        "auth": {
          "key_file": "${env.AZURE_KEY_FILE}",
          "tenant_id": "${env.AZURE_TENANT_ID}",
          "subscription_id": "${env.AZURE_SUBSCRIPTION}",
          "key": "${env.AZURE_KEY}",
          "secret": "${env.AZURE_SECRET}",
          "region": "australiaeast"
        },
        "ssh": {
          "public_key_path": "${env.PUBLIC_KEY_PATH}",
          "private_key_path": "${env.PRIVATE_KEY_PATH}",
          "key_name": "${env.PUBLIC_KEY_NAME}",
          "node_password": "${env.AZURE_NODE_PASSWORD}"
        },
        "create_with": {
          "ex_resource_group": "${env.AZURE_CLOUD_NAME}",
          "ex_storage_account": null
        },
        "provider": {
          "name": "AZURE_ARM",
          "region": "Australia East"
        }
      },
      {
        "provider": {
          "name": "EC2",
          "region": "ap-southeast-2"
        },
        "auth": {
          "key": "${env.AWS_KEY_ID}",
          "secret": "${env.AWS_ACCESS_KEY}"
        },
        "ssh": {
          "public_key_path": "${env.PUBLIC_KEY_PATH}",
          "private_key_path": "${env.PRIVATE_KEY_PATH}",
          "key_name": "${env.PUBLIC_KEY_NAME}"
        },
        "security_group": [
          "all"
        ],
        "key_name": "${env.PUBLIC_KEY_NAME}"
      },
      {
        "provider": {
          "name": "SOFTLAYER",
          "region": "syd01"
        },
        "auth": {
          "key": "${env.SL_USER}",
          "secret": "${env.SL_KEY}"
        },
        "ssh": {
          "public_key_path": "${env.PUBLIC_KEY_PATH}",
          "private_key_path": "${env.PRIVATE_KEY_PATH}",
          "key_name": "${env.PUBLIC_KEY_NAME}"
        }
      },
      {
        "auth": {
          "subscription_id": "${env.AZURE_SUBSCRIPTION}",
          "key_file": "${env.AZURE_KEY_FILE}"
        },
        "ssh": {
          "public_key_path": "${env.PUBLIC_KEY_PATH}",
          "private_key_path": "${env.PRIVATE_KEY_PATH}",
          "key_name": "${env.PUBLIC_KEY_NAME}",
          "node_password": "${env.AZURE_NODE_PASSWORD}"
        },
        "create_with": {
          "ex_cloud_service_name": "${env.AZURE_CLOUD_NAME}"
        },
        "provider": {
          "name": "AZURE",
          "region": "Australia East"
        }
      },
      {
        "provider": {
          "name": "DIGITAL_OCEAN",
          "region": "sgp1"
        },
        "auth": {
          "key": "${env.DIGITAL_OCEAN_KEY}"
        },
        "ssh": {
          "public_key_path": "${env.PUBLIC_KEY_PATH}",
          "private_key_path": "${env.PRIVATE_KEY_PATH}",
          "key_name": "${env.PUBLIC_KEY_NAME}"
        }
      },
      {
        "provider": {
          "name": "VAGRANT",
          "ex_vagrantfile": "/mnt/large_linux/vagrant/edx-fullstack/Vagrantfile",
          "key": "/mnt/large_linux/vagrant/edx-fullstack/Vagrantfile"
        }
      }
    ]
  },
  "register": {
    "/unclustered/any-cluster-ubuntu1604-2-Glaucoma1": [
      {
        "module": "offregister-app-push",
        "kwargs": {
          "GIT_DIR": "/var/www/static/glaucoma-risk-calculator-rest-api",
          "GIT_REPO": "https://github.com/glaucoma-australia/glaucoma-risk-calculator-rest-api",
          "service_name": "glaucoma-risk-calculator-rest-api",
          "skip_reset": false,
          "destroy_node_modules": true,
          "use_sudo": true,
          "node_sudo": false,
          "node_version": "7.10.0",
          "ExecStart": "/bin/bash -c 'PATH={home_dir}/n/bin:$PATH {home_dir}/n/bin/node main.js'",
          "npm_global_packages": [
            "typings",
            "mocha",
            "typescript",
            "bunyan"
          ],
          "post_npm_step": "tsc",
          "nginx": true,
          "app_name": "glaucoma-risk-calculator",
          "DESCRIPTION": "Glaucoma risk calculator frontend and backend",
          "DNS_NAMES": [
            "glaucoma-risk-calculator.complicated.io"
          ],
          "PROXY_ROUTE": "/api",
          "PROXY_PASS": "http://localhost:5465",
          "REST_API_PORT": 5465,
          "NGINX_PORT": 80,
          "WWWROOT": "/var/www/static/glaucoma-risk-calculator-web-frontend-dist/dist",
          "WWWPATH": "/",
          "nginx_secure": "certbot",
          "https_cert_email": "samuel@offscale.io"
        },
        "run_cmds": {
          "op": "==",
          "val": 0
        },
        "type": "fabric"
      },
      {
        "module": "offregister-app-push",
        "kwargs": {
          "GIT_DIR": "/var/www/static/glaucoma-risk-calculator-web-frontend-dist",
          "GIT_REPO": "https://github.com/glaucoma-australia/glaucoma-risk-calculator-web-frontend-dist",
          "use_sudo": true,
          "node_sudo": false,
          "nginx": false
        },
        "type": "fabric"
      }
    ]
  },
  "_comments": [
    {
      "module": "offregister-postgres",
      "kwargs": {
        "users": [
          {
            "name": "glaucoma_user",
            "password": "newPassword"
          }
        ],
        "dbs": [
          "glaucoma_db"
        ],
        "cluster": false
      },
      "type": "fabric"
    },
    {
      "module": "offregister-redis",
      "type": "fabric"
    }
  ],
  "purpose": [
    "any-cluster"
  ],
  "etcd_server": "http://localhost:2379",
  "default_pick": "first"
}
