{
  "name": "Default",
  "description": "Default register strategy",
  "version": "0.0.1",
  "provider": {
    "options": [
      {
        "provider": {
          "name": "EC2",
          "region": "ap-southeast-2"
        },
        "auth": {
          "key": "${env.AWS_KEY_ID}",
          "secret": "${env.AWS_ACCESS_KEY}"
        },
        "ssh": {
          "public_key_path": "${env.PUBLIC_KEY_PATH}",
          "private_key_path": "${env.PRIVATE_KEY_PATH}",
          "key_name": "${env.PUBLIC_KEY_NAME}"
        },
        "security_group": [
          "all"
        ],
        "key_name": "${env.PUBLIC_KEY_NAME}"
      },
      {
        "provider": {
          "name": "SOFTLAYER",
          "region": "syd01"
        },
        "auth": {
          "key": "${env.SL_USER}",
          "secret": "${env.SL_KEY}"
        },
        "ssh": {
          "public_key_path": "${env.PUBLIC_KEY_PATH}",
          "private_key_path": "${env.PRIVATE_KEY_PATH}",
          "key_name": "${env.PUBLIC_KEY_NAME}"
        }
      },
      {
        "provider": {
          "name": "DIGITAL_OCEAN",
          "region": "sgp1"
        },
        "auth": {
          "key": "${env.DIGITAL_OCEAN_KEY}"
        },
        "ssh": {
          "public_key_path": "${env.PUBLIC_KEY_PATH}",
          "private_key_path": "${env.PRIVATE_KEY_PATH}",
          "key_name": "${env.PUBLIC_KEY_NAME}"
        }
      }
    ]
  },
  "register": {
    "/unclustered/*": [
      "offregister-dokku"
    ],
    "_comments": [
      "offregister-ceph",
      "offregister-zookeeper",
      "offregister-mesos",
      "offregister-etcd",
      "offregister-taiga",
      "dokku",
      "tsuru",
      "coreos",
      "deis",
      "flynn",
      "coreos",
      "bosh",
      "consul"
    ]
  },
  "purpose": [
    "any-cluster"
  ],
  "etcd_server": "http://localhost:2379",
  "default_pick": "first"
}