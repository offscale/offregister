{
  "name": "Certbot",
  "description": "Offregister strategy for Certbot (LetsEncrypt; automatic HTTPS certs)",
  "version": "0.0.1",
  "provider": {
    "$ref": "env:OFFAUTH_JSON|offutils.str_from_file | json.loads"
  },
  "register": {
    "/unclustered/any-cluster-ubuntu1604-3-OpenEdX1": [
      {
        "module": "offregister-certbot",
        "cluster_name": "certbot0",
        "args": [],
        "kwargs": {
          "domains": [
            "studio0.complicated.io"
          ],
          "email": "samuel@offscale.io"
        },
        "type": "fabric",
        "_run_cmds": {
          "op": ">",
          "val": 1
        }
      }
    ]
  },
  "purpose": [
    "tensorflow"
  ],
  "etcd_server": "http://localhost:2379",
  "default_pick": "first"
}
